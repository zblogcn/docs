{
  "rules": {
    "no-empty-list": 2,
    "no-empty-code": 2,
    "no-empty-code-lang": 2,
    "no-empty-inlinecode": 2,
    "no-empty-blockquote": 2,
    "no-space-in-link": 2,
    "no-space-in-inlinecode": 2,
    "no-special-characters": 2,
    "no-fullwidth-number": 2,
    "no-trailing-punctuation": 2,
    "no-multiple-space-blockquote": 2,
    "space-round-number": 2,
    "space-round-alphabet": 2,
    "no-long-code": [
      1,
      {
        "length": 500,
        "exclude": []
      }
    ]
  }
}
